#include <bits/stdc++.h>
using namespace std;
#define nl '\n'

void solution() {
    int t; cin >> t;
    while (t--) {
        int n; cin >> n;
        while (n != 0) {
            cout << n << ' ';
            n--;
        }
        cout << nl;
    }
}

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	solution();
	return 0;
}
